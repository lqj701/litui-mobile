{
  "name": "litui-mobile",
  "version": "1.0.0",
  "description": "励推",
  "scripts": {
    "build:dev": "npm run build:clean && cross-env NODE_ENV=development npm run webpack -- --config config/webpack.dev.js --progress --profile",
    "build:prod": "npm run build:clean && cross-env NODE_ENV=production npm run webpack -- --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "build:dll": "npm run webpack -- --config config/webpack.dll.js  --progress --profile ",
    "build:clean": "npm run rimraf -- build",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "npm run webpack-dev-server -- --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run server:dev",
    "test": "jest --colors",
    "coverage": "jest --colors --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "precommit": "lint-staged",
    "prettier": "prettier --write ./**/*.{js,ts,css,scss,json}",
    "lint": "eslint src",
    "lint-staged:js": "NODE_ENV=production eslint src",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js"
  },
  "dependencies": {
    "antd-mobile": "^2.2.3",
    "better-scroll": "^1.12.6",
    "cropperjs": "^1.4.1",
    "echarts": "^4.1.0",
    "humps": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-sortable-hoc": "^0.8.3",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.6.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^3.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chalk": "^2.4.1",
    "compression-webpack-plugin": "^1.1.11",
    "conventional-changelog-cli": "^2.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "enzyme": "^3.4.1",
    "eslint": "^5.3.0",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "happypack": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^2.1.6",
    "prettier": "^1.14.2",
    "redux-logger": "^3.0.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier",
      "lint-staged:js"
    ]
  }
}
